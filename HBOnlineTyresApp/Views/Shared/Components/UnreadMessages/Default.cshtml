@model int;

@if(User.Identity.IsAuthenticated && User.IsInRole("Administrator"))
{
    @if(Model > 0)
    {
         <li class="nav-item">
            <a class=" nav-link text-white" asp-controller="ContactUs" asp-action="Messages">
                <i class=" bi bi-inbox"></i>@Model
            </a>
        </li>
    }
   
}
